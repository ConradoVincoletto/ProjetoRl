{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug ProjetoRl.API",
      "type": "coreclr",
      "request": "launch",      
      "program": "${workspaceFolder}/ProjetoRl.API/bin/Debug/net9.0/ProjetoRl.API.dll",
      "args": [],
      "cwd": "${workspaceFolder}/ProjetoRl.API",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "http://localhost:5116"
      },
      "console": "integratedTerminal",
      "sourceFileMap": {
        "/": "${workspaceFolder}"
      },
      "preLaunchTask": "build"
    }
  ]
}
